{
    "name": "Money Grab",
    "version": "1.0",
    "manifest_version": 2,

    "description": "This is a money grab",
    "icons": {

    },
    
    "background": {
        "scripts": ["background.js"]
    },
    

    "browser_action": {
        "default_title": "Money Grab lmao",
        "default_popup": "popup.html"
    },

    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ]
        }
    ],

    "permissions": [
        "http://*/*", "https://*/*", "activeTab", "tabs", "identity"
    ]

}